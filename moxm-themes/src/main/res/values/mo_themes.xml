<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Base application theme. -->
    <style name="Theme.Moxm" parent="Theme.AppCompat">
        <!-- colorPrimary is used for the default action bar background -->
        <item name="colorPrimary">@color/mo_primary_material_dark</item>

        <!-- colorPrimaryDark is used for the status bar -->
        <item name="colorPrimaryDark">@color/mo_primary_material_dark</item>

        <!-- colorAccent is used as the default value for colorControlActivated
             which is used to tint widgets -->
        <item name="colorAccent">@color/mo_accent_material_dark</item>
        <item name="actionBarSize">@dimen/mo_action_bar_default_height_material</item>
        <!-- 去除ActionBar下面阴影 -->
        <item name="android:windowContentOverlay">@null</item>

        <!-- You can also set colorControlNormal, colorControlActivated
             colorControlHighlight & colorSwitchThumbNormal. -->

        <!-- Toolbar styles -->
        <item name="toolbarStyle">@style/Widget.Moxm.Toolbar</item>
        <!-- homeAsUp 按钮 -->
        <item name="toolbarNavigationButtonStyle">@style/Widget.Moxm.Toolbar.Button.Navigation</item>
        <item name="homeAsUpIndicator">@drawable/mo_ic_ab_back_mtrl_am_alpha</item>
        <!--<item name="borderlessButtonStyle">@style/Widget.Moxm.Button.Borderless</item>-->
        <item name="actionBarStyle">@style/Widget.Moxm.ActionBar.Solid</item>
        <item name="actionButtonStyle">@style/Widget.Moxm.ActionButton</item>


        <item name="colorControlNormal">?android:attr/textColorSecondary</item>
        <item name="colorControlActivated">?attr/colorAccent</item>
        <item name="colorControlHighlight">@color/ripple_material_dark</item>
        <item name="colorButtonNormal">@color/mo_button_material_dark</item>
        <item name="colorSwitchThumbNormal">@color/switch_thumb_material_dark</item>

        <!-- Window colors -->
        <item name="android:colorForeground">@color/bright_foreground_material_dark</item>
        <item name="android:colorForegroundInverse">@color/bright_foreground_material_light</item>
        <item name="android:colorBackground">@color/mo_background_material_dark</item>
        <item name="android:colorBackgroundCacheHint">@color/abc_background_cache_hint_selector_material_dark</item>
        <item name="android:disabledAlpha">@dimen/abc_disabled_alpha_material_dark</item>
        <item name="android:backgroundDimAmount">0.6</item>
        <item name="android:windowBackground">@color/mo_background_material_dark</item>

        <!-- divider -->
        <item name="dividerVertical">@color/mo_list_divider_dark</item>
        <item name="dividerHorizontal">@color/mo_list_divider_dark</item>

        <!-- Text colors -->
        <item name="android:textColorPrimary">@color/mo_primary_text_material_light</item>
        <item name="android:textColorPrimaryInverse">@color/mo_primary_text_material_dark</item>
        <item name="android:textColorPrimaryDisableOnly">@color/mo_primary_text_disable_only_material_dark</item>
        <item name="android:textColorSecondary">@color/mo_secondary_text_material_light</item>
        <item name="android:textColorSecondaryInverse">@color/mo_secondary_text_material_dark</item>
        <item name="android:textColorTertiary">@color/mo_secondary_text_material_light</item>
        <item name="android:textColorTertiaryInverse">@color/mo_secondary_text_material_dark</item>
        <item name="android:textColorHint">@color/mo_hint_foreground_material_light</item>
        <item name="android:textColorHintInverse">@color/mo_hint_foreground_material_dark</item>
        <item name="android:textColorHighlight">@color/mo_highlighted_text_material_dark</item>
        <item name="android:textColorLink">@color/mo_link_text_material_dark</item>

        <!-- Text styles -->
        <item name="android:textAppearance">@style/TextAppearance.Moxm</item>
        <item name="android:textAppearanceInverse">@style/TextAppearance.Moxm.Inverse</item>
        <item name="android:textAppearanceLarge">@style/TextAppearance.Moxm.Large</item>
        <item name="android:textAppearanceLargeInverse">@style/TextAppearance.Moxm.Large.Inverse</item>
        <item name="android:textAppearanceMedium">@style/TextAppearance.Moxm.Medium</item>
        <item name="android:textAppearanceMediumInverse">@style/TextAppearance.Moxm.Medium.Inverse</item>
        <item name="android:textAppearanceSmall">@style/TextAppearance.Moxm.Small</item>
        <item name="android:textAppearanceSmallInverse">@style/TextAppearance.Moxm.Small.Inverse</item>

        <!-- List attributes -->
        <item name="textAppearanceListItem">@style/TextAppearance.Moxm.Subhead</item>
        <item name="textAppearanceListItemSmall">@style/TextAppearance.Moxm.Subhead</item>
        <item name="listPreferredItemHeight">48dp</item>
        <item name="listPreferredItemHeightSmall">32dp</item>
        <item name="listPreferredItemHeightLarge">64dp</item>
        <item name="listPreferredItemPaddingLeft">@dimen/mo_list_item_padding_horizontal_material</item>
        <item name="listPreferredItemPaddingRight">@dimen/mo_list_item_padding_horizontal_material</item>
        <item name="android:listViewStyle">@style/Widget.Moxm.ListView</item>
        <item name="android:listDivider">?attr/dividerVertical</item>

    </style>
    <style name="Theme.Moxm.NoActionBar" parent="Theme.Moxm">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>


    <style name="Widget.Moxm.Button.Borderless" parent="Widget.AppCompat.Button.Borderless">
    </style>
    <!-- homeAsUpIndicator 按钮 -->
    <style name="Widget.Moxm.Toolbar.Button.Navigation" parent="Widget.AppCompat.Toolbar.Button.Navigation">
        <item name="android:minWidth">36dp</item>
        <item name="android:scaleType">center</item>
        <item name="android:background">?attr/selectableItemBackground</item>
        <item name="android:paddingLeft">@dimen/mo_action_bar_navigation_padding_start_material</item>
    </style>
    <!-- Toolbar -->
    <style name="Widget.Moxm.Toolbar" parent="Widget.AppCompat.Toolbar">
        <item name="displayOptions">showTitle|homeAsUp</item>
        <item name="titleTextAppearance">@style/TextAppearance.Widget.Moxm.Toolbar.Title</item>
        <item name="subtitleTextAppearance">@style/TextAppearance.Widget.Moxm.Toolbar.Subtitle</item>
        <item name="android:minHeight">?attr/actionBarSize</item>
        <item name="titleMargins">0dp</item>
        <item name="maxButtonHeight">?attr/actionBarSize</item>
        <item name="collapseIcon">?attr/homeAsUpIndicator</item>
        <item name="navigationIcon">?attr/homeAsUpIndicator</item>
        <item name="contentInsetStart">@dimen/mo_action_bar_content_inset_material</item>
    </style>
    <!-- Toolbar 标题样式 -->
    <style name="TextAppearance.Widget.Moxm.Toolbar.Title" parent="TextAppearance.Moxm.Widget.ActionBar.Title"><!-- 参照 parent="TextAppearance.Widget.AppCompat.Toolbar.Title" -->
    </style>
    <!-- ActionBar 子标题样式 -->
    <style name="TextAppearance.Widget.Moxm.Toolbar.Subtitle" parent="TextAppearance.Moxm.Widget.ActionBar.Subtitle"><!-- 参照 parent="TextAppearance.Widget.AppCompat.Toolbar.Subtitle" -->
        <item name="android:textSize">@dimen/mo_text_size_subtitle_material_toolbar</item>
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>
    <!-- ActionBar 样式 -->
    <style name="Widget.Moxm.ActionBar.Solid" parent="Widget.AppCompat.ActionBar.Solid">
        <item name="displayOptions">showTitle|homeAsUp</item><!-- none|useLogo|showHome|homeAsUp|showTitle|showCustom|disableHome -->
        <item name="titleTextStyle">@style/TextAppearance.Moxm.Widget.ActionBar.Title</item>
        <item name="subtitleTextStyle">@style/TextAppearance.Moxm.Widget.ActionBar.Subtitle</item>

        <item name="actionButtonStyle">@style/Widget.Moxm.ActionButton</item>
        <item name="actionOverflowButtonStyle">@style/Widget.Moxm.ActionButton.Overflow</item>

        <item name="android:gravity">center_vertical</item>
        <item name="contentInsetStart">@dimen/mo_action_bar_content_inset_material</item>
        <item name="contentInsetEnd">@dimen/mo_action_bar_content_inset_material</item>
        <item name="elevation">0dp</item>
    </style>
    <!-- ActionBar 标题样式 -->
    <style name="TextAppearance.Moxm.Widget.ActionBar.Title" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textSize">@dimen/mo_text_size_title_material</item>
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>
    <!-- ActionBar 子标题样式 -->
    <style name="TextAppearance.Moxm.Widget.ActionBar.Subtitle" parent="TextAppearance.AppCompat.Widget.ActionBar.Subtitle">
        <item name="android:textSize">@dimen/mo_text_size_subtitle_material_toolbar</item>
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>

    <style name="Widget.Moxm.ActionButton" parent="Widget.AppCompat.ActionButton">
        <item name="android:paddingLeft">8dip</item>
        <item name="android:paddingRight">8dip</item>
        <item name="android:minWidth">@dimen/mo_action_button_min_width_material</item>
        <item name="android:minHeight">@dimen/mo_action_button_min_height_material</item>
    </style>
    <style name="Widget.Moxm.ActionButton.Overflow" parent="Widget.AppCompat.ActionButton.Overflow">
        <item name="android:paddingLeft">@dimen/mo_action_bar_overflow_padding_start_material</item>
        <item name="android:paddingRight">@dimen/mo_action_bar_overflow_padding_end_material</item>
    </style>

    <!-- Text styles -->
    <style name="TextAppearance.Moxm" parent="TextAppearance.AppCompat">
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:textColorHint">?android:textColorHint</item>
        <item name="android:textColorHighlight">?android:textColorHighlight</item>
        <item name="android:textColorLink">?android:textColorLink</item>
        <item name="android:textSize">@dimen/mo_text_size_body_1_material</item>
    </style>
    <style name="TextAppearance.Moxm.Inverse">
        <item name="android:textColor">?android:attr/textColorPrimaryInverse</item>
        <item name="android:textColorHint">?android:attr/textColorHintInverse</item>
    </style>
    <style name="TextAppearance.Moxm.Large">
        <item name="android:textSize">@dimen/mo_text_size_large_material</item>
        <item name="android:textColor">?android:attr/textColorPrimary</item>
    </style>
    <style name="TextAppearance.Moxm.Large.Inverse">
        <item name="android:textColor">?android:attr/textColorPrimaryInverse</item>
        <item name="android:textColorHint">?android:attr/textColorHintInverse</item>
    </style>
    <style name="TextAppearance.Moxm.Medium">
        <item name="android:textSize">@dimen/mo_text_size_medium_material</item>
        <item name="android:textColor">?android:attr/textColorSecondary</item>
    </style>
    <style name="TextAppearance.Moxm.Medium.Inverse">
        <item name="android:textColor">?android:attr/textColorSecondaryInverse</item>
        <item name="android:textColorHint">?android:attr/textColorHintInverse</item>
    </style>
    <style name="TextAppearance.Moxm.Small">
        <item name="android:textSize">@dimen/mo_text_size_small_material</item>
        <item name="android:textColor">?android:attr/textColorTertiary</item>
    </style>
    <style name="TextAppearance.Moxm.Small.Inverse">
        <item name="android:textColor">?android:attr/textColorTertiaryInverse</item>
        <item name="android:textColorHint">?android:attr/textColorHintInverse</item>
    </style>
    <!-- List styles -->
    <style name="TextAppearance.Moxm.Subhead" parent="TextAppearance.AppCompat.Subhead">
        <item name="android:textSize">@dimen/mo_text_size_subhead_material</item>
        <item name="android:textColor">?android:textColorPrimary</item>
    </style>
    <style name="Widget.Moxm.ListView" parent="@android:style/Widget.ListView">
        <item name="android:textAppearance">?android:attr/textAppearance</item>
        <item name="android:listSelector">@android:color/transparent</item><!-- ?attr/listChoiceBackgroundIndicator -->
        <item name="android:cacheColorHint">?android:attr/colorBackgroundCacheHint</item>
        <item name="android:divider">?android:attr/listDivider</item>
        <item name="android:dividerHeight">1px</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:fadingEdgeLength">0dp</item>
    </style>


    <!-- 参考代码 -->
    <style name="Base.Moxm.Theme.AppCompat" parent="Base.V7.Theme.AppCompat">
        <item name="windowActionBar">true</item>
        <item name="windowActionBarOverlay">false</item>

        <!-- Used by MediaRouter -->
        <item name="isLightTheme">false</item>

        <item name="selectableItemBackground">@drawable/abc_item_background_holo_dark</item>
        <item name="selectableItemBackgroundBorderless">?attr/selectableItemBackground</item>
        <item name="homeAsUpIndicator">@drawable/abc_ic_ab_back_mtrl_am_alpha</item>

        <item name="dividerVertical">@drawable/abc_list_divider_mtrl_alpha</item>
        <item name="dividerHorizontal">@drawable/abc_list_divider_mtrl_alpha</item>

        <!-- Action Bar Styles -->
        <item name="actionBarTabStyle">@style/Widget.AppCompat.ActionBar.TabView</item>
        <item name="actionBarTabBarStyle">@style/Widget.AppCompat.ActionBar.TabBar</item>
        <item name="actionBarTabTextStyle">@style/Widget.AppCompat.ActionBar.TabText</item>
        <item name="actionButtonStyle">@style/Widget.AppCompat.ActionButton</item>
        <item name="actionOverflowButtonStyle">@style/Widget.AppCompat.ActionButton.Overflow</item>

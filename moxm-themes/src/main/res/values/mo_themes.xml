        <item name="dialogTheme">@style/Theme.AppCompat.Dialog</item>
        <item name="dialogPreferredPadding">@dimen/abc_dialog_padding_material</item>

        <item name="alertDialogTheme">@style/Theme.AppCompat.Dialog.Alert</item>
